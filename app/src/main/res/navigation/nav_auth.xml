<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_auth"
    app:startDestination="@id/joinNowFragment">


    <fragment
        android:id="@+id/joinNowFragment"
        android:name="com.aspk.aspk.ui.auth.JoinNowFragment"
        android:label="JoinNowFragment" >

        <action
            android:id="@+id/action_joinNowFragment_to_authFragment"
            app:destination="@id/authFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/authFragment"
        android:name="com.aspk.aspk.ui.auth.AuthFragment"
        android:label="AuthFragment" />
</navigation>